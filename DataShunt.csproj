    </ItemGroup>

    <ItemGroup>
      <Reference Include="Bitter, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
        <HintPath>Bitter.dll</HintPath>
      </Reference>
      <Reference Include="FauFau, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
        <HintPath>FauFau.dll</HintPath>
      </Reference>
      <Reference Include="SharpCompress, Version=0.22.0.0, Culture=neutral, PublicKeyToken=afb0a02973931d96">
        <HintPath>SharpCompress.dll</HintPath>
      </Reference>
    </ItemGroup>

</Project>
